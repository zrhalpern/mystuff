Connect-ExchangeOnline -UserPrincipalName "my_email@domain.com"
Get-MailboxFolderPermission -Identity "target_user@domain.com" # To see current access rights and for which users.
Add-MailBoxFolderPermission -Identity "target_user@domain.com:\Calendar" -User "user_who_needs_access@domain.com" -AccessRights Editor -SharingPermissionFlags Delegate,CanViewPrivateItems
